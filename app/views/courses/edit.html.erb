<h1>Editing Course</h1>

<%= form_for :course, url: course_path(@course), method: :patch do |f| %>
<p style="color:black">
    <%= f.label :code %><br>
    <%= f.text_field :code %>
  </p>
 
  <p style="color:black">
    <%= f.label :name %><br>
    <%= f.text_area :name %>
  </p>
 
  <p style="color:black">
    <%= f.submit :提交%>
  </p>
<% end %>

<%= link_to 'Back', courses_path %>